{
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/api/index",
        "destination": "/api/index.js"
      },
      {
        "source": "/api/proxy/(.*)",
        "destination": "/api/proxy.js"
      }
    ]
  }
